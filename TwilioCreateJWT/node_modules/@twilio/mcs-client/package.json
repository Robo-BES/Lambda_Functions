{
  "name": "@twilio/mcs-client",
  "version": "0.5.1",
  "description": "Twilio Media Content Service client library",
  "license": "MIT",
  "author": "Twilio",
  "main": "./dist/lib.js",
  "browser": "./dist/browser.js",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist/docs",
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.map",
    "dist/post-install.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf ./coverage ./dist",
    "lint": "npx eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts test/**/*.ts --fix",
    "test:unit": "env FORCE_COLOR=1 NODE_ENV=test npx jest test/unit",
    "test:integration": "env FORCE_COLOR=1 NODE_ENV=test npx jest test/integration",
    "build": "npx rollup -c",
    "ssri": "yarn ssri:bundle && yarn ssri:min",
    "ssri:bundle": "openssl dgst -sha256 -binary ./dist/twilio-mcs-client.js | openssl base64 -A | (printf \"sha256-\" && cat)  > ./dist/twilio-conversations.sri",
    "ssri:min": "openssl dgst -sha256 -binary ./dist/twilio-mcs-client.min.js | openssl base64 -A | (printf \"sha256-\" && cat) > ./dist/twilio-conversations.min.sri",
    "sourceMapReport": "yarn sourceMapReport:html && sourceMapReport:json",
    "sourceMapReport:html": "npx source-map-explorer@2.5.2 ./dist/lib.js --html ./dist/lib.report.html",
    "sourceMapReport:json": "npx source-map-explorer@2.5.2 ./dist/lib.js --json ./dist/lib.report.json",
    "ci": "yarn clean && yarn lint && yarn build && yarn ssri",
    "postinstall": "if [ -f ./dist/post-install.js ]; then node ./dist/post-install.js; fi"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.5",
    "@twilio/declarative-type-validator": "^0.1.9",
    "@twilio/operation-retrier": "^4.0.5",
    "core-js": "^3.17.3",
    "loglevel": "^1.7.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.7.4",
    "@types/qs": "6.9.4",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "async": "^3.1.0",
    "async-test-tools": "^1.0.7",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^8.6.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "ink-docstrap": "^1.3.2",
    "isomorphic-form-data": "^2.0.0",
    "jest": "^27.0.3",
    "jsdoc": "~3.5.5",
    "jsdoc-strip-async-await": "^0.1.0",
    "ngrok": "^3.2.5",
    "prettier": "^2.3.0",
    "rollup": "^2.51.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-polyfill-node": "^0.6.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^8.3.0",
    "twilio": "^3.55.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=10"
  }
}
